//q11
public class Main{
	public static void main(String[] args) {
		int a = 10;
		int b = 20;
		int add = a+b;
		int sub = a-b;
		int mul = a*b;
		int div = a/b;
		System.out.println("Addition: "+add);
		System.out.println("Subtraction: "+sub);
		System.out.println("Multipication: "+mul);
		System.out.println("Division: "+div);

	}
}
